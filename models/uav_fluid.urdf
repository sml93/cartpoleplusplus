<?xml version="1.0"?>
<robot name="uav">
    <!-- Colors --> 
    <material name="black"> 
        <color rgba="0 0 0 1"/>
    </material>
    <material name="blue"> 
        <color rgba="0.2 0.2 0.8 1"/>
    </material>

  <link name="uavLink">
    <contact>
      <lateral_friction value="1.0"/>
      <rolling_friction value="0.0"/>
      <inertia_scaling value="3.0"/>
      <contact_cfm value="0.0"/>
      <contact_erp value="1.0"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.5"/>
      <geometry>
        <box size="0.2 0.2 0.05"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.2 0.2 0.03"/>
      </geometry>
    </collision>
  </link>

  <link name="fluidLink">
    <contact>
      <lateral_friction value="1.0"/>
      <rolling_friction value="0.0"/>
      <inertia_scaling value="3.0"/>
      <contact_cfm value="0.0"/>
      <contact_erp value="1.0"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.0"/>
      <mass value="5"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 -0.3"/>
      <geometry>
        <box size="0.02 0.02 0.5"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.02 0.02 0.5"/>
      </geometry>
    </collision>
  </link>

  <joint name="actuation_joint" type="revolute">
    <parent link="uavLink"/>
    <child link="fluidLink"/>
    <origin xyz="0 0 0.55" rpy="0 0 0"/>
    <limit lower="-0.785" upper="0.349" effort="10" velocity="3"/>
    <axis xyz="0 1 0"/>
  </joint>

 </robot>
